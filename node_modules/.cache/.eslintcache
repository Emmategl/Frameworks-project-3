n      <h2>Total: ${calculateTotal(cartItems).toFixed(2)}</h2>\n    </Wrapper>\n  );\n};\n\nexport default Cart;\n","/Users/emmateglbraender/Documents/ITU/2semester/Frameworks and Architectures for the Web/Mini projects/Mini project 3/hot_liquids/src/Components/Cart/Cart.styles.ts",[],"/Users/emmateglbraender/Documents/ITU/2semester/Frameworks and Architectures for the Web/Mini projects/Mini project 3/hot_liquids/src/Components/CartItem/CartItem.tsx",["161"],"import Button from '@material-ui/core/Button';\n// Types\nimport { CartItemType } from '../CartItemType';\n// Styles\nimport { Wrapper } from './CartItem.styles';\nimport './Buttons.css';\nimport {MyButton} from '../../App';\nimport { CSSProperties } from 'react';\n\ntype Props = {\n  item: CartItemType;\n  addToCart: (clickedItem: CartItemType) => void;\n  removeFromCart: (productId: number) => void;\n  removeFromCart2: (productId: number) => void;\n};\n\nconst myStyles: CSSProperties = {\n  textAlign: \"center\"\n}\n\nconst CartItem: React.FC<Props> = ({ item, addToCart, removeFromCart, removeFromCart2 }) => (\n  <Wrapper>\n    <div>\n      <h3>{item.name}</h3>\n      <div className='information'>\n        <p>Price: ${item.price}</p>\n        <p>Total: ${(item.quantity * item.price).toFixed(2)}</p>\n      </div>\n      <div className='buttons'>\n        <MyButton \n          size='small'\n          disableElevation\n          variant='contained'\n          onClick={() => addToCart(item)}\n        >\n          +\n        </MyButton>\n        <p style={myStyles}>Quantity: {item.quantity}</p>\n        <MyButton\n          size='small'\n          disableElevation\n          variant='contained'\n          onClick={() => removeFromCart(item.productId)}\n        >\n          -\n        </MyButton>\n        <MyButton\n        size='small'\n        disableElevation\n        variant='contained'\n        onClick={() => removeFromCart2(item.productId)}\n        >\n        Remove\n      </MyButton>\n      </div>\n    </div>\n    <img src={item.img_path} alt={item.name} />\n  </Wrapper>\n);\n\nexport default CartItem;\n","/Users/emmateglbraender/Documents/ITU/2semester/Frameworks and Architectures for the Web/Mini projects/Mini project 3/hot_liquids/src/Components/CartItem/CartItem.styles.ts",[],"/Users/emmateglbraender/Documents/ITU/2semester/Frameworks and Architectures for the Web/Mini projects/Mini project 3/hot_liquids/src/Components/Item/Item.tsx",[],"/Users/emmateglbraender/Documents/ITU/2semester/Frameworks and Architectures for the Web/Mini projects/Mini project 3/hot_liquids/src/Components/Item/Item.styles.ts",[],"/Users/emmateglbraender/Documents/ITU/2semester/Frameworks and Architectures for the Web/Mini projects/Mini project 3/hot_liquids/src/Pages/Login/FormContext.tsx",["162","163","164","165","166"],"import React, { useState } from \"react\";\nimport { BrowserRouter, NavLink, Route, Switch } from \"react-router-dom\";\nimport { LoginForm } from \"./LoginForm\";\nimport PropTypes from \"prop-types\";\n\nexport interface LoginInfo {\n    firstName: string,\n    lastName: string,\n    email: string,\n  }\n\n export const init = {\n    firstName: \"\",\n    lastName: \"\",\n    email: \"\"\n  }\nexport interface IFormContext {\n    login: LoginInfo;\n    updateName: (firstName:string, lastName:string, email:string) => void\n  }\n\n// create context, but there is no default value - set it to undefined.\nexport const FormContext = React.createContext<IFormContext | undefined>(\n    undefined\n  );\n\n  export const { Consumer } = FormContext;\n\n  export function App(){\n    const [login, updateName] = useState<LoginInfo>(init)\n    const updateNameFun = (firstName: string, lastName: string, email: string) => {\n        updateName((prev) => ({...prev, ...{email:email, firstname:firstName, lastName:lastName}})) \n      }\n\n      return (\n        <FormContext.Provider value={{ login: login, updateName:updateNameFun}}>\n          <LoginForm/>\n        </FormContext.Provider>\n      );\n\n  }\n","/Users/emmateglbraender/Documents/ITU/2semester/Frameworks and Architectures for the Web/Mini projects/Mini project 3/hot_liquids/src/Pages/Login/LoginForm.tsx",[],"/Users/emmateglbraender/Documents/ITU/2semester/Frameworks and Architectures for the Web/Mini projects/Mini project 3/hot_liquids/src/Pages/Login/Context.tsx",[],{"ruleId":"167","severity":1,"message":"168","line":7,"column":10,"nodeType":"169","messageId":"170","endLine":7,"endColumn":23},{"ruleId":"167","severity":1,"message":"171","line":7,"column":25,"nodeType":"169","messageId":"170","endLine":7,"endColumn":39},{"ruleId":"172","replacedBy":"173"},{"ruleId":"174","replacedBy":"175"},{"ruleId":"167","severity":1,"message":"176","line":2,"column":8,"nodeType":"169","messageId":"170","endLine":2,"endColumn":12},{"ruleId":"167","severity":1,"message":"177","line":4,"column":8,"nodeType":"169","messageId":"170","endLine":4,"endColumn":19},{"ruleId":"167","severity":1,"message":"178","line":7,"column":10,"nodeType":"169","messageId":"170","endLine":7,"endColumn":26},{"ruleId":"167","severity":1,"message":"179","line":7,"column":28,"nodeType":"169","messageId":"170","endLine":7,"endColumn":42},{"ruleId":"167","severity":1,"message":"180","line":2,"column":10,"nodeType":"169","messageId":"170","endLine":2,"endColumn":14},{"ruleId":"172","replacedBy":"181"},{"ruleId":"174","replacedBy":"182"},{"ruleId":"167","severity":1,"message":"183","line":1,"column":10,"nodeType":"169","messageId":"170","endLine":1,"endColumn":18},{"ruleId":"167","severity":1,"message":"184","line":2,"column":10,"nodeType":"169","messageId":"170","endLine":2,"endColumn":18},{"ruleId":"167","severity":1,"message":"185","line":2,"column":30,"nodeType":"169","messageId":"170","endLine":2,"endColumn":45},{"ruleId":"167","severity":1,"message":"186","line":8,"column":19,"nodeType":"169","messageId":"170","endLine":8,"endColumn":31},{"ruleId":"167","severity":1,"message":"187","line":8,"column":20,"nodeType":"169","messag