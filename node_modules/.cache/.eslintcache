onstruction from './Pages/UnderConstruction/UnderConstruction';\nimport ScrollUp from './Components/ScrollUp';\nimport AboutUs from './Pages/AboutUs/AboutUs';\nimport './App.css';\nimport PageNotFound from './Pages/PageNotFound';\n\nfunction App(){\n  return (\n    <>\n     <BrowserRouter>\n     <ScrollUp />\n    <div className=\"main-container\">\n    <Nav/>\n    <Switch>\n                <Route exact path=\"/\" component={Home} />\n                <Route path=\"/allproducts\" component={ProductList} />\n                <Route path=\"/coffees\" component={Coffees} />\n                <Route path=\"/teas\" component={Teas} />\n                <Route path=\"/login\" component={LoginForm} />\n                <Route path=\"/aboutus\" component={AboutUs} />\n                <Route path=\"/underconstruction\" component={UnderConstruction} />\n                <Route path=\"/404\" component={PageNotFound} />\n                <Route path=\"/:productId\" render={(props) => <ProductDescription {...props} />} />\n                <Route component={PageNotFound} />\n    </Switch>\n    </div>\n    <div className=\"footers\">\n    <Footer/>\n    </div>\n    </BrowserRouter>\n    </>\n  );\n}\n\nexport default App;\n","/Users/emmateglbraender/Documents/ITU/2semester/Frameworks and Architectures for the Web/Mini projects/Mini project 3/hot_liquids/src/App.styles.ts",[],"/Users/emmateglbraender/Documents/ITU/2semester/Frameworks and Architectures for the Web/Mini projects/Mini project 3/hot_liquids/src/Pages/Home/Components/pictureBanner.tsx",[],["151","152"],"/Users/emmateglbraender/Documents/ITU/2semester/Frameworks and Architectures for the Web/Mini projects/Mini project 3/hot_liquids/src/Pages/Home/Components/topPictures.tsx",[],"/Users/emmateglbraender/Documents/ITU/2semester/Frameworks and Architectures for the Web/Mini projects/Mini project 3/hot_liquids/src/Pages/Coffees/Coffees.tsx",["153","154","155"],"/Users/emmateglbraender/Documents/ITU/2semester/Frameworks and Architect