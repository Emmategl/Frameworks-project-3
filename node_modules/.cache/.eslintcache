",[],"/Users/emmateglbraender/Documents/ITU/2semester/Frameworks and Architectures for the Web/Mini projects/Mini project 3/hot_liquids/src/Components/UserName.tsx",["121","122","123","124","125"],"import React from \"react\";\nimport { Link } from \"react-router-dom\";\nimport pic1 from \"../Images/Homepage/Farmer.jpg\";\nimport pic2 from \"../Images/Homepage/Flavours.jpg\";\nimport pic3 from \"../Images/Homepage/Retailer.jpg\";\nimport pic4 from \"../Images/Homepage/World.jpg\";\nimport '../style.css';\n\nfunction Name() {\n    return <p>Welcome you are already logged in</p>\n  }\n  \n\nfunction UserName() {\n    return (\n        <p>Welcome {Name()}</p>\n    );\n  }\n\n  export default UserName;\n","/Users/emmateglbraender/Documents/ITU/2semester/Frameworks and Architectures for the Web/Mini projects/Mini project 3/hot_liquids/src/Components/Register.tsx",["126","127"],"import React from 'react';\n\nconst useStateWithLocalStorage = (localStorageKey: string) => {\n    const [value, setValue] = React.useState(\n      localStorage.getItem(localStorageKey) || ''\n    );\n   \n    React.useEffect(() => {\n      localStorage.setItem(localStorageKey, value);\n    }, [value]);\n   \n    return [value, setValue];\n  };\n \nconst App = () => {\n  const [value, setValue] = React.useState(\n    localStorage.getItem('myValueInLocalStorage') || ''\n  );\n\n\n \n  React.useEffect(() => {\n    localStorage.setItem('myValueInLocalStorage', value);\n  }, [value]);\n \n  const onChange = (event: { target: { value: any; }; }) => setValue(event.target.value);\n \n  return (\n    <div>\n      <h1>Hello React with Local Storage!</h1>\n \n      <input value={value} type=\"text\" onChange={onChange} />\n \n      <p>{value}</p>\n    </div>\n  );\n};\n \nexport default App;",["128","129"],"/Users/emmateglbraender/Documents/ITU/2semester/Frameworks and Architectures for the Web/Mini projects/Mini project 3/hot_liquids/src/App.styles.ts",[],"/Users/emmateglbraender/Documents/ITU/2semester/Fr